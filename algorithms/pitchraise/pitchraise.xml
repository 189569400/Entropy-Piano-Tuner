<?xml version="1.0"?>
<algorithm year="2015" author="Haye Hinrichsen">
    <name>
        <string>Pitch Raise</string>
        <string lang="de">Frequenzanhebung (pitch raise)</string>
    </name>

    <description>
        <string>This algorithm generates an approximate tuning which is needed for a pitch raise. For details please refer to the user manual.</string>
        <string lang="de">Dieser Algorithmus erzeugt eine ungefähre Stimmkurve, die man für eine Frequenzanhebung (pitch raise) benötigt. Einzelheiten dazu finden Sie in der Bedienungsanleitung.</string>
    </description>

    <param id="concertPitch" type="double" min="430" max="450" default="440" precision="0.5">
        <label>
            <string>Concert pitch</string>
            <string lang="de">Kammerton</string>
        </label>
        <description>
            <string>The desired concert pitch.</string>
            <string lang="de">Der gewünschte Kammerton.</string>
        </description>
    </param>

    <param id="dummyList" type="list" default="entry_2">
        <label>
            <string>Dummy list</string>
            <string lang="de">Dummy-Liste</string>
        </label>
        <description>
            <string>Select any entry.</string>
            <string lang="de">Wähle einen beliebigen Eintrag.</string>
        </description>
        <entry value="entry_1">
            <string>Entry 1</string>
            <string lang="de">Eintrag 1</string>
        </entry>
        <entry value="entry_2">
            <string>Entry 2</string>
            <string lang="de">Eintrag 2</string>
        </entry>
    </param>
</algorithm>

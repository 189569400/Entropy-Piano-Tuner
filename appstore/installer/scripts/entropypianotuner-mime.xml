<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/ept">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">Entropy Piano Tuner File</comment>
    <comment xml:lang="de">Entropy Piano Tuner Datei</comment>
    <magic priority="10">
      <match type="string" offset="10:200" value="entropyPianoTunerFile"/>
    </magic>
    <glob pattern="*.ept"/>
  </mime-type>
</mime-info>
